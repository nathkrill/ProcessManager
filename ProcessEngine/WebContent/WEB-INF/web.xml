<?xml version="1.0" encoding="UTF-8"?>
<web-app id="ProcessEngine" version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <listener>
    <listener-class>nl.adaptivity.process.engine.servlet.PEContextListener</listener-class>
  </listener>
  <servlet>
    <servlet-name>processEngine</servlet-name>
    <servlet-class>nl.adaptivity.process.engine.servlet.ServletProcessEngine</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>processEngine</servlet-name>
    <url-pattern>/*</url-pattern>
  </servlet-mapping>

</web-app>
<processModel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://adaptivity.nl/ProcessEngine/ ProcessEngine.xsd"
              xmlns="http://adaptivity.nl/ProcessEngine/"
              xmlns:jbi="http://adaptivity.nl/ProcessEngine/activity" name="testProcessModel 2">
    <start id="start" />
    <activity id="ac1" predecessor="start" name="ac1">
        <result name="name" path="/values/user/text()" />
        <result name="name2"><user xmlns=""><fullname><jbi:value xpath="/values/user/text()"/></fullname></user></result>
        <message serviceNS="http://adaptivity.nl/userMessageHandler"
            serviceName="userMessageHandler" endpoint="internal"
            operation="postTask" url="/PEUserMessageHandler/internal">
            <Envelope xmlns="http://www.w3.org/2003/05/soap-envelope"
                encodingStyle="http://www.w3.org/2003/05/soap-encoding">
                <Body>
                    <postTask xmlns="http://adaptivity.nl/userMessageHandler">
                        <repliesParam>
                            <jbi:element value="endpoint" />
                        </repliesParam>
                        <taskParam>
                            <task summary="Task Foo">
                                <jbi:attribute name="remotehandle"
                                    value="handle" />
                                <jbi:attribute name="instancehandle"
                                    value="instancehandle" />
                                <jbi:attribute name="owner"
                                    value="owner" />
                                <item type="label"
                                    value="Please enter some info for task foo" />
                                <item name="user" type="text"
                                    label="Your name" />
                            </task>
                        </taskParam>
                    </postTask>
                </Body>
            </Envelope>
        </message>
    </activity>
    <activity id="ac2" predecessor="ac1" name="ac2">
        <message serviceNS="http://adaptivity.nl/userMessageHandler"
            serviceName="userMessageHandler" endpoint="internal"
            operation="postTask" url="/PEUserMessageHandler/internal">
            <Envelope xmlns="http://www.w3.org/2003/05/soap-envelope"
                encodingStyle="http://www.w3.org/2003/05/soap-encoding">
                <Body>
                    <postTask xmlns="http://adaptivity.nl/userMessageHandler">
                        <repliesParam>
                            <jbi:element value="endpoint" />
                        </repliesParam>
                        <taskParam>
                            <task summary="Task Bar">
                                <jbi:attribute name="remotehandle"
                                    value="handle" />
                                <jbi:attribute name="instancehandle"
                                    value="instancehandle" />
                                <jbi:attribute name="owner"
                                    value="owner" />
                            </task>
                        </taskParam>
                    </postTask>
                </Body>
            </Envelope>
        </message>
    </activity>
    <activity id="ac3" predecessor="ac1" name="ac3">
        <message serviceNS="http://adaptivity.nl/userMessageHandler"
            serviceName="userMessageHandler" endpoint="internal"
            operation="postTask" url="/PEUserMessageHandler/internal">
            <Envelope xmlns="http://www.w3.org/2003/05/soap-envelope"
                encodingStyle="http://www.w3.org/2003/05/soap-encoding">
                <Body>
                    <postTask xmlns="http://adaptivity.nl/userMessageHandler">
                        <repliesParam>
                            <jbi:element value="endpoint" />
                        </repliesParam>
                        <taskParam>
                            <task summary="Task Baz1">
                                <jbi:attribute name="remotehandle"
                                    value="handle" />
                                <jbi:attribute name="instancehandle"
                                    value="instancehandle" />
                                <jbi:attribute name="owner"
                                    value="owner" />
                            </task>
                        </taskParam>
                    </postTask>
                </Body>
            </Envelope>
        </message>
    </activity>
    <activity id="ac5" predecessor="ac3" name="ac5">
        <message serviceNS="http://adaptivity.nl/userMessageHandler"
            serviceName="userMessageHandler" endpoint="internal"
            operation="postTask" url="/PEUserMessageHandler/internal">
            <Envelope xmlns="http://www.w3.org/2003/05/soap-envelope"
                encodingStyle="http://www.w3.org/2003/05/soap-encoding">
                <Body>
                    <postTask xmlns="http://adaptivity.nl/userMessageHandler">
                        <repliesParam>
                            <jbi:element value="endpoint" />
                        </repliesParam>
                        <taskParam>
                            <task summary="Task Baz2">
                                <jbi:attribute name="remotehandle"
                                    value="handle" />
                                <jbi:attribute name="instancehandle"
                                    value="instancehandle" />
                                <jbi:attribute name="owner"
                                    value="owner" />
                            </task>
                        </taskParam>
                    </postTask>
                </Body>
            </Envelope>
        </message>
    </activity>
    <join id="j1" min="2" max="2">
        <predecessor>ac2</predecessor>
        <predecessor>ac5</predecessor>
    </join>
    <activity id="ac4" predecessor="j1" name="ac4">
        <message serviceNS="http://adaptivity.nl/userMessageHandler"
            serviceName="userMessageHandler" endpoint="internal"
            operation="postTask" url="/PEUserMessageHandler/internal">
            <Envelope xmlns="http://www.w3.org/2003/05/soap-envelope"
                encodingStyle="http://www.w3.org/2003/05/soap-encoding">
                <Body>
                    <postTask xmlns="http://adaptivity.nl/userMessageHandler">
                        <repliesParam>
                            <jbi:element value="endpoint" />
                        </repliesParam>
                        <taskParam>
                            <task summary="Task Last">
                                <jbi:attribute name="remotehandle"
                                    value="handle" />
                                <jbi:attribute name="instancehandle"
                                    value="instancehandle" />
                                <jbi:attribute name="owner"
                                    value="owner" />
                            </task>
                        </taskParam>
                    </postTask>
                </Body>
            </Envelope>
        </message>
    </activity>
    <end predecessor="ac4" id="end" />
</processModel>

<?xml version="1.0" encoding="UTF-8"?>
<fileset-config file-format-version="1.2.0" simple-config="false">
    <fileset name="test" enabled="true" check-config-name="Test default" local="false">
        <file-match-pattern match-pattern="src/net/devrieze/test/.*\.java$" include-pattern="true"/>
    </fileset>
    <fileset name="non-test" enabled="true" check-config-name="Paul's default" local="false">
        <file-match-pattern match-pattern="^src/.*" include-pattern="true"/>
        <file-match-pattern match-pattern="src/net/devrieze/test/" include-pattern="false"/>
    </fileset>
</fileset-config>

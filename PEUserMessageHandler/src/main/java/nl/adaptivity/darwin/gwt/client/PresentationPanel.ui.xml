<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:my="urn:import:nl.adaptivity.darwin.gwt.client">
  <ui:style>
    .presentationContainer {
	     margin: 0px auto;
	     width: 90%;
	  }
  </ui:style>
  <g:FlowPanel stylePrimaryName="{style.presentationContainer}">
    <my:PresentationGroupPanel ui:field="groupPanel"></my:PresentationGroupPanel>
  
  </g:FlowPanel>
</ui:UiBinder>